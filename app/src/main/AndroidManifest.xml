<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.fishingtest">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Controllers.EditUserActivity"></activity>
        <activity android:name=".Controllers.RegisterActivity" />
        <activity android:name=".Controllers.LogInActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Controllers.HomePageActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".Controllers.AdminActivity"
            android:screenOrientation="portrait"
            android:parentActivityName=".Controllers.HomePageActivity"/>
    </application>

</manifest>